<% include ../partials/header %>
<div class="jumbotron">
<table class="table">     
    <thead>
        <tr>
            <th>ลำดับ</th>
            <th>เลขสมาชิก</th>
            <th>วันที่</th>
            
        </tr>
    </thead>
    <tbody id="myTable">
        <%type.forEach(function(bookq){ %>
        <tr>
            <td scope="row"> <%= bookq.TID %></td>
            <td><%= bookq.Type %></td>
            <td><%= bookq.Detill %></td>
            
            <td><a class="btn btn-primary" href="/type/<%= bookq.TID %>" role="button">เลื่อนไปอีก 30 นาที</a></td>
        </tr>
        <% }); %>
    </tbody>
</table>
<script>
    $(document).ready(function () {
        $("#myInput").on("keyup", function () {
            var value = $(this).val().toLowerCase();
            $("#myTable tr").filter(function () {
                $(this).toggle($(this).text().toLowerCase().indexOf(value) > -1)
            });
        });
    });
</script>
</div>
<% include ../partials/footer %>